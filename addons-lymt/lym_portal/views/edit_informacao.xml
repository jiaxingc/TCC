<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <template id="edit_informacao" name="Editar Informação">
            <t t-call="website.layout">
                <t t-set="additional_title">Edição de Informação</t>
                <div id="wrap" style="font-size: 160%;">
                    <section class="s_website_form pt16 pb16 o_colored_level" data-vcss="001" data-snippet="s_website_form" data-name="Form">
                        <div class="container">
                            <form action="/myportal/informacoes/editing" method="post" enctype="multipart/form-data" class="js_website_submit_form" data-model_name="">
                                <div class="s_website_form_rows row s_col_no_bgcolor">

                                    <input type="hidden" class="d-none form-control s_website_form_input" name="id" placeholder="id" id="id1" t-att-value="event_sponsor.id"/>

                                    <div class="form-group s_website_form_field col-12 s_website_form_required" data-type="char" data-name="Field">
                                        <div class="row s_col_no_resize s_col_no_bgcolor">
                                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="website_description1">
                                                <span class="s_website_form_label_content">Descrição</span>
                                            </label>
                                            <div class="col-sm">
                                                <input type="text" class="form-control s_website_form_input" name="website_description" placeholder="Descrição" id="website_description1" style="font-size: 100%;" t-att-value="event_sponsor.website_description"/>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group s_website_form_field col-12 s_website_form_required" data-type="char" data-name="Field">
                                        <div class="row s_col_no_resize s_col_no_bgcolor">
                                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="email1">
                                                <span class="s_website_form_label_content">Email</span>
                                            </label>
                                            <div class="col-sm">
                                                <input type="text" class="form-control s_website_form_input" name="email" placeholder="Email" id="email1" style="font-size: 100%;" t-att-value="event_sponsor.email"/>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group s_website_form_field col-12 s_website_form_required" data-type="char" data-name="Field">
                                        <div class="row s_col_no_resize s_col_no_bgcolor">
                                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="url1">
                                                <span class="s_website_form_label_content">URL</span>
                                            </label>
                                            <div class="col-sm">
                                                <input type="text" class="form-control s_website_form_input" name="url" placeholder="URL" id="url1" style="font-size: 100%;" t-att-value="event_sponsor.url"/>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group s_website_form_field col-12 s_website_form_required" data-type="char" data-name="Field">
                                        <div class="row s_col_no_resize s_col_no_bgcolor">
                                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="youtube1">
                                                <span class="s_website_form_label_content">Youtube</span>
                                            </label>
                                            <div class="col-sm">
                                                <input type="text" class="form-control s_website_form_input" name="youtube" placeholder="Youtube" id="youtube1" style="font-size: 100%;" t-att-value="event_sponsor.youtube"/>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group s_website_form_field col-12 s_website_form_required" data-type="char" data-name="Field">
                                        <div class="row s_col_no_resize s_col_no_bgcolor">
                                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="linkedin1">
                                                <span class="s_website_form_label_content">Linkedin</span>
                                            </label>
                                            <div class="col-sm">
                                                <input type="text" class="form-control s_website_form_input" name="linkedin" placeholder="Linkedin" id="linkedin1" style="font-size: 100%;" t-att-value="event_sponsor.linkedin"/>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group s_website_form_field col-12 s_website_form_required" data-type="char" data-name="Field">
                                        <div class="row s_col_no_resize s_col_no_bgcolor">
                                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="facebook1">
                                                <span class="s_website_form_label_content">Facebook</span>
                                            </label>
                                            <div class="col-sm">
                                                <input type="text" class="form-control s_website_form_input" name="facebook" placeholder="Facebook" id="facebook1" style="font-size: 100%;" t-att-value="event_sponsor.facebook"/>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group s_website_form_field col-12 s_website_form_required" data-type="char" data-name="Field">
                                        <div class="row s_col_no_resize s_col_no_bgcolor">
                                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="instagram1">
                                                <span class="s_website_form_label_content">Instagram</span>
                                            </label>
                                            <div class="col-sm">
                                                <input type="text" class="form-control s_website_form_input" name="instagram" placeholder="Instagram" id="instagram1" style="font-size: 100%;" t-att-value="event_sponsor.instagram"/>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group s_website_form_field col-12 s_website_form_required" data-type="char" data-name="Field">
                                        <div class="row s_col_no_resize s_col_no_bgcolor">
                                            <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="twitter1">
                                                <span class="s_website_form_label_content">Twitter</span>
                                            </label>
                                            <div class="col-sm">
                                                <input type="text" class="form-control s_website_form_input" name="twitter" placeholder="Twitter" id="twitter1" style="font-size: 100%;" t-att-value="event_sponsor.twitter"/>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group col-12 s_website_form_submit" data-name="Submit Button">
                                        <div style="width: 200px;" class="s_website_form_label" />
                                        <button type="submit" class="btn btn-primary" style="font-size: 160%;">Enviar</button>
                                        <t t-if="error">
                                            <span id="s_website_form_result" class="text-danger ml8">
                                                <i class="fa fa-close mr4" role="img" aria-label="Erro" title="Erro"></i>
                                                <t t-raw="error"/>
                                            </span>
                                        </t>
                                    </div>

                                </div>
                            </form>
                        </div>
                    </section>
                </div>
            </t>
        </template>

    </data>
</odoo>