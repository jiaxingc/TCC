<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="lym_myportal_Agendamento" name="Agendamento">
            <t t-call="website.layout">
                <t t-set="additional_title">Agendamento</t>
                <t t-call="lym_portal.Agendamento" />
            </t>
        </template>

        <template id="Agendamento" name="Agendamento">
            <div id="wrap" class="oe_structure oe_empty">
                <section class="s_website_form pt16 pb16 o_colored_level" data-vcss="001" data-snippet="s_website_form" data-name="Form" style="">
                    <div class="container">
                        <form action="/website_form/" method="post" enctype="multipart/form-data" class="o_mark_required" data-mark="*" data-success-mode="redirect" data-success-page="/contactus-thank-you" data-model_name="mail.mail">
                            <div class="s_website_form_rows row s_col_no_bgcolor">
                                <div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required  " data-type="char" data-name="Field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="qckbi4f8508">
                                            <span class="s_website_form_label_content">Nome:</span>
                                            <span class="s_website_form_mark">                     *</span>
                                        </label>
                                        <div class="col-sm">
                                            <input type="text" class="form-control s_website_form_input" name="Nome:" required="1" placeholder="Nome Completo " id="qckbi4f8508" />
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group s_website_form_field col-12 s_website_form_custom   " data-type="tel" data-name="Field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="3ldgqg7vth4">
                                            <span class="s_website_form_label_content">Telefone</span>
                                        </label>
                                        <div class="col-sm">
                                            <input type="tel" class="form-control s_website_form_input" name="Telefone" placeholder="(dd) xxxx-xxxxx" id="3ldgqg7vth4" />
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group s_website_form_field col-12  s_website_form_model_required  " data-type="char" data-name="Field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="2yrfpvfis0y">
                                            <span class="s_website_form_label_content">Email</span>
                                            <span class="s_website_form_mark">              *</span>
                                        </label>
                                        <div class="col-sm">
                                            <input type="text" class="form-control s_website_form_input" name="email_from" required="true" placeholder="admin@example.com" id="2yrfpvfis0y" />
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group s_website_form_field col-12 s_website_form_custom   " data-type="date" data-name="Field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="dnkh60l7kgo">
                                            <span class="s_website_form_label_content">Data do Agendamento </span>
                                        </label>
                                        <div class="col-sm">
                                            <div class="s_website_form_date input-group date" id="datepicker030322608058681144" data-target-input="nearest">
                                                <input type="text" class="form-control datetimepicker-input s_website_form_input" data-target="#datepicker030322608058681144" name="Data do Agendamento " placeholder="DD/MM/AA" id="dnkh60l7kgo" />
                                                <div class="input-group-append" data-target="#datepicker030322608058681144" data-toggle="datetimepicker">
                                                    <div class="input-group-text">
                                                        <i class="fa fa-calendar" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group s_website_form_field col-12  s_website_form_model_required  " data-type="char" data-name="Field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="8j86134ps75">
                                            <span class="s_website_form_label_content">Serviço desejado </span>
                                            <span class="s_website_form_mark">     *</span>
                                        </label>
                                        <div class="col-sm">
                                            <input type="text" class="form-control s_website_form_input" name="subject" required="true" placeholder="" id="8j86134ps75" />
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group s_website_form_field col-12 s_website_form_custom s_website_form_required  " data-type="text" data-name="Field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px" for="1mg62m88p1o">
                                            <span class="s_website_form_label_content">Descrição</span>
                                            <span class="s_website_form_mark">       *</span>
                                        </label>
                                        <div class="col-sm">
                                            <textarea class="form-control s_website_form_input" name="Descrição" required="1" placeholder="" id="1mg62m88p1o" rows="3" />
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group s_website_form_field col-12    s_website_form_dnone" data-name="Field">
                                    <div class="row s_col_no_resize s_col_no_bgcolor">
                                        <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px">
                                            <span class="s_website_form_label_content" />
                                            <span title="Separe os endereços de e-mail com uma vírgula.">
                                                <i class="fa fa-info-circle" />
                                            </span>
                                        </label>
                                        <div class="col-sm">
                                            <input type="hidden" class="form-control s_website_form_input" name="email_to" value="info@yourcompany.example.com" />
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group col-12 s_website_form_submit" data-name="Submit Button">
                                    <div style="width: 200px;" class="s_website_form_label" />
                                    <a href="#" role="button" class="btn btn-primary btn-lg s_website_form_send o_default_snippet_text">Enviar</a>
                                    <span id="s_website_form_result" />
                                </div>
                            </div>
                        </form>
                    </div>
                </section>
            </div>
        </template>
    </data>
</odoo>
