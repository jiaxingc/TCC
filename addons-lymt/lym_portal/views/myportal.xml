<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <template id="lym_myportal" name="Portal do Gerente">
            <t t-call="website.layout">
                <t t-set="additional_title">Portal do Gerente</t>
                <t t-call="lym_portal.myportal" />
            </t>
        </template>

        <template id="myportal" name="Portal do Gerente">
            <section class="myportal">
                <div class="container">
                    <div class="row">
                        <div class="col-12">
                            <h1 class="align-center">
                                <br />
                                <strong style="color:#007bff;">Portal do Gerente</strong>
                                <br />
                            </h1>
                        </div>
                    </div>
                    <div class="row">
                        <div class="twelve columns">
                            <ul class="ca-menu">
                                <li>
                                    <a href="#">
                                        <span class="ca-icon">
                                            <i class="fa fa-heart"></i>
                                        </span>
                                        <div class="ca-content">
                                            <h2 class="ca-main">
                                                Responsive
                                                <br />
                                                Design
                                            </h2>
                                            <h3 class="ca-sub">Across all major devices</h3>
                                        </div>
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <span class="ca-icon">
                                            <i class="fa fa-bullhorn"></i>
                                        </span>
                                        <div class="ca-content">
                                            <h2 class="ca-main">
                                                Friendly
                                                <br />
                                                Documentation
                                            </h2>
                                            <h3 class="ca-sub">Straight to the point</h3>
                                        </div>
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <span class="ca-icon">
                                            <i class="fa fa-user"></i>
                                        </span>
                                        <div class="ca-content">
                                            <h2 class="ca-main">
                                                Alternate
                                                <br />
                                                Home Pages
                                            </h2>
                                            <h3 class="ca-sub">Full slider, boxed or none</h3>
                                        </div>
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <span class="ca-icon">
                                            <i class="fa fa-camera"></i>
                                        </span>
                                        <div class="ca-content">
                                            <h2 class="ca-main">
                                                Filterable
                                                <br />
                                                Portofolio
                                            </h2>
                                            <h3 class="ca-sub">Isotop&amp;PrettyPhoto</h3>
                                        </div>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </section>
            <link rel="stylesheet" href="lym_portal/static/src/css/myportal.css" />
            <link rel="stylesheet" href="lym_portal/static/src/css/style.css" />
            <link rel="stylesheet" href="lym_portal/static/src/css/responsive.css" />
            <link rel="stylesheet" href="lym_portal/static/src/css/blue.css" />
        </template>

        <template id="my_account_link_override" name="Link to frontend portal Override" inherit_id="portal.user_dropdown">
            <xpath expr="//a[@href='/my/home']" position="replace">
                <div class="menu-myportal" />
            </xpath>
        </template>

        <template id="assets_frontend" inherit_id="website.assets_frontend" name="Font">
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/lym_portal/static/src/js/myportal.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/website_myportal_form_editor.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/foundation.min.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/jquery.easing.1.3.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/elasticslideshow.js.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/jquery.carouFredSel-6.0.5-packed.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/jquery.cycle.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/app.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/modernizr.foundation.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/slidepanel.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/scrolltotop.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/hoverIntent.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/superfish.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/responsivemenu.js" />
                <script type="text/javascript" src="/lym_portal/static/src/js/jquery.easing.1.3.js" />
            </xpath>
        </template>
    </data>
</odoo>