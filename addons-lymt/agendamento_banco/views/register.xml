<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template name="inherit_signup" id="inherit_signup" inherit_id="auth_signup.fields">
        <xpath expr="//div[last()]" position="after">
            <div class="form-group field-login">
                <label for="rg">RG</label>
                <input type="text"  id="rg" class="form-control form-control-sm" required="1"/>
            </div>
            <div class="form-group field-login">
                <label for="cpf_cnpj">CPF/CNPJ</label>
                <input type="text" t-att-value="cpf_cnpj" id="cpf_cnpj" class="form-control form-control-sm" required="1"/>
            </div>
            <div class="form-group field-login">
                <label for="phone">Telefone</label>
                <input type="text" name="phone" t-att-value="phone" id="phone" class="form-control form-control-sm" required="1"/>
            </div>
            <div class="form-group field-login">
                <label for="zip">CEP</label>
                <input type="text"  id="zip" class="form-control form-control-sm" required="1"/>
            </div>
        </xpath>
    </template>

</odoo>